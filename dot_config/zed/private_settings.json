{
  "features": {
    "inline_completion_provider": "none"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "auto_update_extensions": {
    "odin": true
  },
  "ui_font_size": 16,
  "unstable.ui_density": "compact",
  "buffer_font_size": 14,
  "buffer_line_height": "standard",
  "buffer_font_weight": 400,
  "buffer_font_features": {},
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Andromeda"
  },
  "vim_mode": true,
  "vertical_scroll_margin": 15,
  "buffer_font_family": "Fira Code",
  "tab_size": 2
}
